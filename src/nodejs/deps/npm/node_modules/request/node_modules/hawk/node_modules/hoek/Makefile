test:
	@node node_packages/lab/bin/lab
test-cov: 
	@node node_packages/lab/bin/lab -r threshold -t 100
test-cov-html:
	@node node_packages/lab/bin/lab -r html -o coverage.html
complexity:
	@node node_packages/complexity-report/src/cli.js -o complexity.md -f markdown lib

.PHONY: test test-cov test-cov-html complexity
